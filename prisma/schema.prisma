datasource db {
  // could be postgresql or mysql
  provider = "postgres"
  url      = env("DB_URL")
}

generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

model Post {
  id        String    @id @default(cuid())
  createdAt DateTime? @default(now()) @map("created_at")
  updatedAt DateTime? @default(now()) @map("updated_at")
  title     String
  published Boolean
  descs     String?
}
